   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "HOURLY_ENTRIES = df.ENTRIES - df.ENTRIES.shift(1)\n",
    "df['HOURLY_ENTRIES'] = HOURLY_ENTRIES.fillna(0)\n",
    "\n",
    "HOURLY_EXITS = df.EXITS - df.EXITS.shift(1)\n",
    "df['HOURLY_EXITS'] = HOURLY_ENTRIES.fillna(0)\n"
   ]
  },
  {
